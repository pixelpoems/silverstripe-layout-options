{
    "name": "pixelpoems/silverstripe-layout-options",
    "type": "silverstripe-vendormodule",
    "description": "Silverstripe Module to attach multiple layout options to pages and elements.",
    "keywords": [
        "silverstripe",
        "layout",
        "extension"
    ],
    "authors": [
        {
            "name": "Bettina BrÃ¶thaler",
            "homepage": "https://www.pixelpoems.com/",
            "role": "Lead Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "silverstripe/cms": ">=4",
        "silverstripe/framework": ">=4",
        "silverstripe/vendor-plugin": ">=1",
        "pixelpoems/silverstripe-selection-field": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Pixelpoems\\LayoutOptions\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "extra": {
        "installer-name": "silverstripe-layout-options",
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "license": "BSD-3-Clause"
}
